{
	"name": "control table",
	"properties": {
		"parameters": {
			"table_storage_account": {
				"type": "String",
				"defaultValue": "medalionlake<suffix>"
			}
		},
		"annotations": [],
		"type": "AzureTableStorage",
		"typeProperties": {
			"connectionString": "DefaultEndpointsProtocol=https;AccountName=@{linkedService().table_storage_account};EndpointSuffix=core.windows.net;",
			"accountKey": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "medalionakv",
					"type": "LinkedServiceReference",
					"parameters": {
						"medalion_keyvault_name": "medalionakv<suffix>"
					}
				},
				"secretName": "medalion-acc-key"
			}
		}
	},
	"type": "Microsoft.Synapse/workspaces/linkedservices"
}